  {
    "name": "WGS84 Standard Ellipsoid",
    "iconUrl": "Widgets/Images/TerrainProviders/Ellipsoid.png",
    "attribution": "",
    "default": true,
    "type": "TerrainLayer",
    "cesium": {
      "type": "Ellipsoid"
    }
  }, 
  {
    "name": "Kalisio Terrain",
    "iconUrl" : "Widgets/Images/TerrainProviders/CesiumWorldTerrain.png",
    "attribution": "High-resolution, mesh-based terrain for the entire globe.\nBy http://www.kalisio.com",
    "type": "TerrainLayer",
    "cesium": {
      "type": "Cesium",
      "url": "http://cesiumterrainserver.kalisio.xyz/tilesets/md15-tiles",
      "requestWaterMask" : "true",
      "requestVertexNormals" : "true"
    }
  },
  {
    "name": "Wind",
    "iconUrl" : "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/wind.jpg",
    "attribution": "Forecast data from <a href=\"http://www.meteofrance.com\">Météo-France</a>",
    "type" : "OverlayLayer",
    "leaflet": {
      "type": "weacast.flowLayer",
      "arguments": [{
        "elements": ["u-wind", "v-wind"],
        "lineWidth": 4,
        "frameRate": 20,
        "particleMultiplier": 0.001,
        "displayOptions": {
          "velocityType": "Wind",
          "position": "bottomright",
          "emptyString": "No wind data",
          "angleConvention": "meteoCW",
          "speedUnit": "m/s"
        }
      }]
    }
  },
  {
    "name": "Gust",
    "iconUrl" : "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/gust.jpg",
    "attribution": "Forecast data from <a href=\"http://www.meteofrance.com\">Météo-France</a>",
    "type" : "OverlayLayer",
    "leaflet": {
      "type": "weacast.scalarLayer",
      "arguments": [{
        "elements": ["gust"],
        "mesh": true
      }]
    }
  },
  {
    "name": "Precipitations",
    "iconUrl" : "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/precipitations.png",
    "attribution": "Forecast data from <a href=\"http://www.meteofrance.com\">Météo-France</a>",
    "type" : "OverlayLayer",
    "leaflet": {
      "type": "weacast.scalarLayer",
      "arguments": [{
        "elements": ["precipitations"],
        "mesh": true,
        "colorMap": "BuPu",
        "colorClasses": [0, 1, 2, 4, 10, 25, 50, 100, 300]
      }]
    }
  },
  {
    "name": "Vigicrues",
    "iconUrl" : "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/vigicrues-icon.png",
    "attribution" : "",
    "type" : "OverlayLayer",
    "leaflet": {
      "type": "geoJson",
      "arguments": [
        "https://s3.eu-central-1.amazonaws.com/kargo/vigicrues.json",
        {
          "realtime": true,
          "interval": 900000,
          "id": "properties.gid"
        }
      ]
    }
  },
  {
    "name": "Téléray",
    "iconUrl" : "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/teleray-icon.jpg",
    "attribution" : "",
    "type" : "OverlayLayer",
    "leaflet": {
      "type": "geoJson",
      "arguments": [
        "https://s3.eu-central-1.amazonaws.com/kargo/teleray.json",
        {
          "realtime": true,
          "interval": 600000,
          "id": "properties.irsnId",
          "container": "markerClusterGroup"
        }
      ]
    }
  },
  {
    "name": "ADS-B",
    "iconUrl": "https://s3.eu-central-1.amazonaws.com/kalisioscope/assets/adsb-icon.jpg",
    "attribution": "",
    "type": "OverlayLayer",
    "leaflet": {
      "type": "geoJson",
      "arguments": [
        "https://s3-eu-west-1.amazonaws.com/gift-backbone-adsb/adsb-airline-one.json",
        {
          "realtime": true,
          "interval": 5000,
          "id": "properties.icao"
        }
      ]
    }
  },
