{
  "name": "kApp",
  "description": "Kalisio App Template",
  "version": "0.1.0",
  "homepage": "https://github.com/kalisio/kApp",
  "keywords": [
    "kalisio"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/kApp.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/kApp/issues"
  },
  "scripts": {
    "prepublish": "npm run build",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:patch": "cd api && npm run release:patch && cd .. && npm version --force patch && npm run publish",
    "release:minor": "cd api && npm run release:minor && cd .. && npm version --force minor && npm run publish",
    "release:major": "cd api && run release:major && cd .. && npm version --force major && npm run publish",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "cd api && npm run test && cd .. && npm run build && npm run lint"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "annyang": "^2.6.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "casl": "^1.0.2",
    "fastclick": "^1.0.6",
    "feathers-authentication-management": "^1.0.0",
    "feathers-client": "^2.2.0",
    "feathers-hooks": "^2.0.1",
    "feathers-reactive": "^0.4.1",
    "jquery": "^3.2.1",
    "leaflet": "^1.0.3",
    "leaflet-basemaps": "^0.2.0",
    "leaflet-filelayer": "^0.6.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-measure": "^2.1.7",
    "leaflet.markercluster": "^1.0.5",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.18.1",
    "quasar-extras": "0.x",
    "quasar-framework": "^0.14.1",
    "roboto-fontface": "^0.7.0",
    "rxjs": "^5.4.1",
    "socket.io-client": "^2.0.1",
    "vue": "~2.3.4",
    "vue-avatar": "^2.0.0",
    "vue-router": "^2.7.0",
    "whatwg-fetch": "^2.0.3"
  },
