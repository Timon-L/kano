    "purify-css": "^1.2.6",
    "shelljs": "^0.7.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "vue-loader": "~12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "~2.3.4",
    "webpack": "^3.5.2",
